---
layout: null
---
[
{% assign items = site.posts | concat: site.pages %}
{% for item in items %}
  {% if item.title %}
    {
      "title": "{{ item.title | escape }}",
      "url": "{{ item.url | absolute_url }}",
      "content": "{{ item.content | strip_html | strip_newlines | escape }}"
    }{% unless forloop.last %},{% endunless %}
  {% endif %}
{% endfor %}
]